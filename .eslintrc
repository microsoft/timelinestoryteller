{
    "extends": "eslint-config-airbnb-es5",
    "rules": {
        "no-var": 0,
        "quotes": [2, "double"],
        "camelcase": 0,
        "no-param-reassign": 0,
        "one-var": 0,
        "indent": [2, 2],
        "func-names": 0,
        "consistent-return": 0,
        "no-inner-declarations": 0,
        "no-undefined": 0,
        "new-cap": 0
    },
    "env": {
        "browser": true,
        "mocha": true
    },
    "globals": {
        "ArrayBuffer": true,
        "Uint8Array": true,
        "Promise": true
    }
}